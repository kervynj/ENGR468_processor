#! c:/iverilog-x64/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000028dfd80 .scope module, "rom_tb" "rom_tb" 2 7;
 .timescale 0 0;
v00000000028de350_0 .var/i "i", 31 0;
v00000000028de3f0_0 .net "instruction", 15 0, v00000000028dcfb0_0;  1 drivers
v00000000028de490_0 .var/i "j", 31 0;
v00000000028a2760_0 .var "oeb", 0 0;
v00000000028a2800_0 .var "pc", 2 0;
S_00000000028dc980 .scope module, "MUT" "ROM" 2 15, 3 5 0, S_00000000028dfd80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "oeb"
    .port_info 1 /OUTPUT 16 "inst"
    .port_info 2 /INPUT 3 "pc"
P_00000000028dff00 .param/l "Awidth" 0 3 6, +C4<00000000000000000000000000000100>;
P_00000000028dff38 .param/l "Dwidth" 0 3 6, +C4<00000000000000000000000000010000>;
P_00000000028dff70 .param/l "Length" 1 3 14, +C4<000000000000000000000000000000010000>;
v00000000028dcfb0_0 .var "inst", 15 0;
v00000000028a2ef0 .array "mem", 15 0, 15 0;
v00000000028dcb00_0 .net "oeb", 0 0, v00000000028a2760_0;  1 drivers
v00000000028de210_0 .net "pc", 2 0, v00000000028a2800_0;  1 drivers
v00000000028de2b0_0 .var "rw", 0 0;
E_00000000028d8f90 .event posedge, v00000000028dcb00_0;
    .scope S_00000000028dc980;
T_0 ;
    %wait E_00000000028d8f90;
    %load/vec4 v00000000028dcb00_0;
    %store/vec4 v00000000028de2b0_0, 0, 1;
    %load/vec4 v00000000028de2b0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v00000000028de210_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v00000000028a2ef0, 4;
    %store/vec4 v00000000028dcfb0_0, 0, 16;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00000000028dfd80;
T_1 ;
    %vpi_call 2 20 "$readmemb", "ROM_instructions.txt", v00000000028a2ef0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00000000028de490_0, 0, 32;
T_1.0 ;
    %load/vec4 v00000000028de490_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %vpi_call 2 26 "$display", "data[%d] = %b", v00000000028de490_0, &A<v00000000028a2ef0, v00000000028de490_0 > {0 0 0};
    %load/vec4 v00000000028de490_0;
    %addi 1, 0, 32;
    %store/vec4 v00000000028de490_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000028a2760_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v00000000028a2800_0, 0, 3;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00000000028de350_0, 0, 32;
T_1.2 ;
    %load/vec4 v00000000028de350_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_1.3, 5;
    %load/vec4 v00000000028de350_0;
    %pad/s 3;
    %store/vec4 v00000000028a2800_0, 0, 3;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000028a2760_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000028a2760_0, 0, 1;
    %load/vec4 v00000000028de350_0;
    %addi 1, 0, 32;
    %store/vec4 v00000000028de350_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_00000000028dfd80;
T_2 ;
    %vpi_call 2 44 "$monitor", "t=%3d, PC=%d, instruction=%b\012", $time, v00000000028a2800_0, v00000000028de3f0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
