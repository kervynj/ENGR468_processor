 .timescale 0 0;
v000000000275cd80_0 .net "cond", 1 0, v00000000027551d0_0;  1 drivers
v000000000275ce20_0 .net "dest", 2 0, v0000000002755270_0;  1 drivers
v000000000275cec0_0 .var "inst", 15 0;
v000000000275cf60_0 .net "opcd", 3 0, v0000000002722980_0;  1 drivers
v000000000275d000_0 .net "source", 2 0, v0000000002722a20_0;  1 drivers
v00000000027af980_0 .net "source2", 3 0, v0000000002722ac0_0;  1 drivers
S_0000000002722760 .scope module, "MUT" "Decoder" 2 25, 3 1 0, S_0000000002755050;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "inst"
    .port_info 1 /OUTPUT 2 "cond"
    .port_info 2 /OUTPUT 4 "opcd"
    .port_info 3 /OUTPUT 3 "dest"
    .port_info 4 /OUTPUT 3 "source"
    .port_info 5 /OUTPUT 4 "source2"
v00000000027551d0_0 .var "cond", 1 0;
v0000000002755270_0 .var "dest", 2 0;
v00000000027228e0_0 .net "inst", 15 0, v000000000275cec0_0;  1 drivers
v0000000002722980_0 .var "opcd", 3 0;
v0000000002722a20_0 .var "source", 2 0;
v0000000002722ac0_0 .var "source2", 3 0;
E_000000000275b070 .event edge, v00000000027228e0_0;
    .scope S_0000000002722760;
T_0 ;
    %wait E_000000000275b070;
    %load/vec4 v00000000027228e0_0;
    %parti/s 2, 14, 5;
    %store/vec4 v00000000027551d0_0, 0, 2;
    %load/vec4 v00000000027228e0_0;
    %parti/s 4, 10, 5;
    %store/vec4 v0000000002722980_0, 0, 4;
    %load/vec4 v00000000027228e0_0;
    %parti/s 3, 7, 4;
    %store/vec4 v0000000002755270_0, 0, 3;
    %load/vec4 v00000000027228e0_0;
    %parti/s 3, 4, 4;
    %store/vec4 v0000000002722a20_0, 0, 3;
    %load/vec4 v00000000027228e0_0;
    %parti/s 4, 0, 2;
    %store/vec4 v0000000002722ac0_0, 0, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0000000002755050;
T_1 ;
    %pushi/vec4 1184, 0, 16;
    %store/vec4 v000000000275cec0_0, 0, 16;
    %delay 5, 0;
    %pushi/vec4 2208, 0, 16;
    %store/vec4 v000000000275cec0_0, 0, 16;
    %delay 5, 0;
    %pushi/vec4 3344, 0, 16;
    %store/vec4 v000000000275cec0_0, 0, 16;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0000000002755050;
T_2 ;
    %vpi_call 2 22 "$monitor", "t= %d, inst= %d,opcd= %d, dest= %d, source= %d, source2= %d", $time, v000000000275cd80_0, v000000000275cf60_0, v000000000275ce20_0, v000000000275d000_0, v00000000027af980_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
